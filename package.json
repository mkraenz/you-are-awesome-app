{
    "name": "you-are-awesome-app",
    "version": "0.0.1",
    "description": "You are Awesome App! Daily Motivation Up!",
    "license": "UNLICENSED",
    "repository": {
        "type": "git",
        "url": "https://github.com/proSingularity/you-are-awesome-app.git"
    },
    "private": true,
    "scripts": {
        "postinstall": "jetify",
        "android": "react-native run-android",
        "ios": "react-native run-ios",
        "web": "expo start --web",
        "dev": "expo start",
        "build": "tsc",
        "build:watch": "tsc -w",
        "sanity-check": "npm install && yarn build && yarn test && yarn expo start",
        "start": "react-native start",
        "test": "jest",
        "test:watch": "jest --watch --coverage=false --changedSince=origin/master",
        "test:debug": "jest --onlyChanged --watch --coverage=false",
        "test:coverage": "jest",
        "updateSnapshots": "jest --updateSnapshot --coverage=false",
        "deploy:prod": "expo publish",
        "deploy:test": "expo publish --release-channel test"
    },
    "dependencies": {
        "casual-browserify": "^1.5.19-2",
        "expo": "^35.0.0",
        "expo-permissions": "^7.0.0",
        "lodash": "^4.17.15",
        "luxon": "^1.21.1",
        "native-base": "^2.13.8",
        "react": "16.8.3",
        "react-dom": "16.8.3",
        "react-native": "0.59.10",
        "react-native-elements": "^1.2.3",
        "react-native-gesture-handler": "^1.3.0",
        "react-native-modal-datetime-picker": "^7.6.0",
        "react-native-reanimated": "^1.2.0",
        "react-native-screens": "^1.0.0-alpha.23",
        "react-native-unimodules": "~0.5.4",
        "react-native-web": "^0.11.7",
        "react-navigation": "^4.0.10",
        "react-navigation-drawer": "^2.2.2",
        "react-navigation-stack": "^1.9.1",
        "react-redux": "^7.1.1",
        "redux": "^4.0.4",
        "redux-saga": "^1.1.1",
        "uuid": "^3.3.3"
    },
    "devDependencies": {
        "@babel/core": "^7.6.0",
        "@types/jest": "^24.0.19",
        "@types/lodash": "^4.14.147",
        "@types/luxon": "^1.15.2",
        "@types/react": "^16.9.11",
        "@types/react-native": "^0.57.65",
        "@types/react-redux": "^7.1.4",
        "@types/react-test-renderer": "^16.9.1",
        "@types/redux": "^3.6.0",
        "@types/redux-mock-store": "^1.0.1",
        "@types/redux-saga": "^0.10.5",
        "@types/uuid": "^3.4.5",
        "babel-preset-expo": "^7.0.0",
        "codecov": "^3.6.1",
        "expo-cli": "^3.4.1",
        "jest": "^24.9.0",
        "jest-expo": "^35.0.0",
        "jetifier": "^1.6.4",
        "redux-mock-store": "^1.5.3",
        "typescript": "^3.7.2"
    }
}
