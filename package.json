{
    "name": "you-are-awesome-app",
    "version": "0.0.1",
    "description": "You are Awesome App! Daily Motivation Up!",
    "license": "UNLICENSED",
    "repository": {
        "type": "git",
        "url": "https://github.com/proSingularity/you-are-awesome-app.git"
    },
    "scripts": {
        "postinstall": "jetify",
        "android": "react-native run-android",
        "ios": "react-native run-ios",
        "web": "expo start --web",
        "dev": "expo start",
        "dev:clear-cache": "expo start --clear",
        "build": "tsc",
        "build:watch": "tsc -w",
        "sanity-check": "npm install && yarn build && yarn test && yarn expo start",
        "start": "react-native start",
        "test": "jest",
        "test:watch": "jest --watch --coverage=false --changedSince=origin/master",
        "test:debug": "jest --onlyChanged --watch --coverage=false",
        "test:coverage": "jest",
        "test:update-snapshot": "jest --updateSnapshot --coverage=false",
        "assert:prod-config": "ts-node ./scripts/assertProdConfig.ts",
        "build-and-publish:expo:android": "expo build:android --type apk",
        "publish:playstore": "expo upload:android --latest --use-submission-service",
        "deploy:ota:prod": "NODE_ENV=prod node ./scripts/expo-publish-with-version.js",
        "deploy:ota:test": "expo publish --release-channel test"
    },
    "dependencies": {
        "@react-native-community/datetimepicker": "2.4.0",
        "@react-native-community/masked-view": "0.1.10",
        "@react-native-community/netinfo": "5.9.2",
        "@react-navigation/material-top-tabs": "^5.2.16",
        "@react-navigation/native": "^5.5.0",
        "@react-navigation/stack": "^5.9.0",
        "axios": "^0.20.0",
        "expo": "^38.0.9",
        "expo-firebase-analytics": "~2.4.1",
        "expo-firebase-core": "~1.1.1",
        "expo-localization": "~8.2.1",
        "expo-notifications": "~0.3.3",
        "expo-status-bar": "^1.0.0",
        "i18next": "^19.4.5",
        "lodash": "^4.17.20",
        "luxon": "^1.24.1",
        "react": "~16.11.0",
        "react-dom": "~16.11.0",
        "react-i18next": "^11.5.0",
        "react-native": "https://github.com/expo/react-native/archive/sdk-38.0.2.tar.gz",
        "react-native-gesture-handler": "~1.6.0",
        "react-native-modal-datetime-picker": "^8.7.1",
        "react-native-paper": "^3.10.1",
        "react-native-reanimated": "~1.9.0",
        "react-native-safe-area-context": "~3.0.7",
        "react-native-screens": "~2.9.0",
        "react-native-tab-view": "^2.15.1",
        "react-native-web": "~0.14.0",
        "react-native-webview": "9.4.0",
        "react-redux": "^7.2.0",
        "redux": "^4.0.5",
        "redux-persist": "^6.0.0",
        "redux-saga": "^1.1.3",
        "traverse": "^0.6.6"
    },
    "devDependencies": {
        "@babel/core": "^7.8.6",
        "@testing-library/jest-native": "^3.3.0",
        "@testing-library/react-native": "^7.0.2",
        "@types/jest": "^26.0.4",
        "@types/lodash": "^4.14.155",
        "@types/luxon": "^1.24.3",
        "@types/mockdate": "^2.0.0",
        "@types/react": "~16.9.41",
        "@types/react-native": "~0.62.13",
        "@types/react-redux": "^7.1.9",
        "@types/react-test-renderer": "^16.9.3",
        "@types/redux-mock-store": "^1.0.2",
        "@types/traverse": "^0.6.32",
        "@types/uuid": "^8.0.0",
        "babel-preset-expo": "^8.2.3",
        "expo-cli": "^3.27.6",
        "jest": "^26.4.0",
        "jest-date-mock": "^1.0.8",
        "jest-expo": "~38.0.2",
        "jsonfile": "^6.0.1",
        "mockdate": "^3.0.2",
        "react-test-renderer": "^16.13.1",
        "redux-mock-store": "^1.5.4",
        "shelljs": "^0.8.4",
        "ts-node": "^9.0.0",
        "typescript": "~3.9.5"
    },
    "private": true
}
