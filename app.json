{
    "name": "yaawesomeapp",
    "displayName": "You are Awesome App! Daily Motivation Up!",
    "expo": {
        "name": "You are Awesome App!",
        "slug": "you-are-awesome-app",
        "orientation": "portrait",
        "icon": "./assets/google-play-store/logo.png",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "privacy": "public",
        "version": "1.4.0",
        "entryPoint": "node_modules/expo/AppEntry.js",
        "platforms": ["ios", "android", "web"],
        "android": {
            "package": "eu.kraenz.youareawesomeapp",
            "permissions": [],
            "googleServicesFile": "./google-services.json",
            "versionCode": 6,
            "useNextNotificationsApi": true
        },
        "ios": {
            "supportsTablet": true
        },
        "description": "Join our community that tells you HOW AWESOME YOU ARE. Spread the awesomeness by sending awesome messages to other people around the globe and feel proud to help them reach their goals."
    }
}
